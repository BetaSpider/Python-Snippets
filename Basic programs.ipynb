    "# Compound interest"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "482a0008",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "enter the principle amount65000\n",
      "enter the no of years6\n",
      "enter the rate of interest4\n",
      "The amount is: 82245.73620224002\n",
      "The compund Interest for the given values is: 17245.73620224002\n"
     ]
    }
   ],
   "source": [
    "p=int(input(\"enter the principle amount\"))\n",
    "n=int(input(\"enter the no of years\"))\n",
    "r=int(input(\"enter the rate of interest\"))\n",
    "A=p*(1+r/100)**n\n",
    "print(\"The amount is:\",A)\n",
    "CI=A-p\n",
    "print(\"The compund Interest for the given values is:\",CI)"
   ]
