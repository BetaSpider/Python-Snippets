   "source": [
    "# swap the first and last element in the list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "1e8afbec",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "List before swapping:\n",
      " [75, 23, 56, 78, 65, 98]\n",
      "The list after swapping: [98, 23, 56, 78, 65, 75]\n"
     ]
    }
   ],
   "source": [
    "myList=[75,23,56,78,65,98]\n",
    "print(\"List before swapping:\\n\", myList)\n",
    "def swap(myList):\n",
    "    myList[0],myList[-1]=myList[-1],myList[0]\n",
    "    return myList\n",
    "print(\"The list after swapping:\",swap(myList))"
   ]
  },
